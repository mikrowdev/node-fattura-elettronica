{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "declaration": true,
      "lib": [ "ES2022" ],
      "module": "CommonJS",
      "resolveJsonModule": true,
      "noImplicitReturns": true,
      "outDir": "dist",
      "removeComments": true,
      "sourceMap": true,
      "strict": true,
      "target": "ES2022",
      "useUnknownInCatchVariables": false,
      "typeRoots": [ "@types", "./node_modules/@types" ]
    },
    "exclude":["build", "build/**"]
  }
  